{
  "name": "starterkit",
  "version": "0.1.0",
  "description": "Claude CLI plugin for small AI development teams",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "starterkit": "dist/cli.js"
  },
  "files": [
    "dist",
    "core",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && node -e \"const fs=require('fs');const path=require('path');const copyTemplates=(src,dst)=>{fs.mkdirSync(dst,{recursive:true});fs.readdirSync(src).filter(f=>f.endsWith('.hbs')).forEach(f=>fs.copyFileSync(path.join(src,f),path.join(dst,f)))};copyTemplates('src/tracker/templates','dist/tracker/templates');copyTemplates('src/commands/publish/templates','dist/commands/publish/templates')\"",
    "prepare": "npm run build",
    "test": "vitest run",
    "dev": "tsc --watch"
  },
  "keywords": ["claude", "cli", "plugin", "ai", "collaboration"],
  "author": "Jeff Lin",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "simple-git": "^3.20.0",
    "handlebars": "^4.7.8",
    "pm2": "^5.3.0"
  }
}
